<section class="member-form">
  <h1 class="form-label">Login</h1>
  <%= form_tag '/login' do %>

    <%= text_field_tag :email, nil, placeholder: "example@example.com" %>
    <br>
    <%= password_field_tag(:password, nil, placeholder: 'Password') %>
    <br>
    <%= submit_tag "Submit" %>

  <% end %>
</section>